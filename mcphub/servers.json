{
	"mcpServers": {
		"fetch": {
			"command": "uvx",
			"args": ["mcp-server-fetch"]
		},
		"time": {
			"command": "uvx",
			"args": ["mcp-server-time", "--local-timezone=America/New_York"]
		},
		"sequentialthinking": {
			"command": "mise",
			"args": [
				"x",
				"npm:@modelcontextprotocol/server-sequential-thinking",
				"--",
				"mcp-server-sequential-thinking"
			]
		},
		"context7": {
			"command": "mise",
			"args": ["x", "npm:@upstash/context7-mcp", "--", "context7-mcp"]
		}
	}
}
