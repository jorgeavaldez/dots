{
	"mcpServers": {
		"fetch": {
			"command": "uvx",
			"args": ["mcp-server-fetch"]
		},
		"context7": {
			"command": "mise",
			"args": ["x", "npm:@upstash/context7-mcp", "--", "context7-mcp"]
		},
		"playwright": {
			"type": "stdio",
			"command": "mise",
			"args": [
				"x",
				"npm:@playwright/mcp@latest",
				"--",
				"mcp-server-playwright"
			],
			"env": {}
		}
	}
}
